<button class="btn btn-large btn-success upvote" type="button" id="${id}" onclick="upvote()">
	<i class="icon-thumbs-up icon-white"></i>
</button>
<input class="centered" style="width: 3em; height: 2.8em; margin: 5px;"
	type="text" size="1" value="${votes}" disabled>
<button class="btn btn-large btn-danger downvote" type="button" id="${id}" onclick="downvote()">
	<i class="icon-thumbs-down icon-white"></i>
</button>